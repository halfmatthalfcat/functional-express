{
  "name": "functional-express",
  "version": "1.0.0",
  "description": "Functional helpers for dealing with Express.js routes.",
  "bugs": {
    "url": "https://github.com/halfmatthalfcat/functional-express/issues"
  },
  "homepage": "https://github.com/halfmatthalfcat/functional-express#readme",
  "author": "Matt Oliver <mjo2089@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halfmatthalfcat/functional-express.git"
  },
  "keywords": [
    "expressjs",
    "nodejs"
  ],
  "scripts": {
    "build": "gulp build",
    "test": "gulp unit",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "create-patch": "gulp bump-patch",
    "create-minor": "gulp bump-minor",
    "create-major": "gulp bump-major"
  },
  "dependencies": {
    "@types/express": "^4.0.39",
    "class-transformer": "^0.1.8",
    "class-transformer-validator": "^0.4.0",
    "class-validator": "^0.7.3",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.2",
    "@types/jest": "^21.1.8",
    "coveralls": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.8.0",
    "gulp-git": "^2.4.2",
    "gulp-jest-cli": "github:halfmatthalfcat/gulp-jest-cli#0.0.4",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "jasmine": "^2.8.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-jasmine2": "^21.2.1",
    "jest-mock-express": "^0.1.1",
    "pre-commit": "^1.2.2",
    "semver": "^5.4.1",
    "ts-jest": "^21.2.4",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "uncommitted": "^1.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ],
    "mapCoverage": true
  }
}
